{
  "setup": [
    "make clean",
    "make -j ${yuclid.binary.values}"
  ],
  "env": {
    "LD_PRELOAD": "libjemalloc.so",
    "NRUNS": 10
  },
  "trial": "./cmd.sh ./${yuclid.binary}.sh ${yuclid.args} > tmp/${yuclid.#} 2>&1",
  "metrics": {
    "time1": "cat tmp/${yuclid.#} | grep 'ms' | grep -oE '[0-9]+(\\.[0-9]+)?'",
    "timeN": "cat tmp/${yuclid.#} | grep 'ms' | grep -oE '[0-9]'"
  },
  "space": {
    "machine": [
      "fix",
      "allagash"
    ],
    "binary": [
      "bfs_topdown",
      "bfs_bottomup"
    ],
    "nthreads:py": "list(range(1, 3))",
    "args": [
      {
        "name": "twitter",
        "value": "-f twitter.bin -r 12 -n $NRUNS"
      },
      {
        "name": "kron",
        "value": "-f kron.bin -r 1 -n $NRUNS"
      }
    ],
    "pinning": [
        "balanced",
        "scatter"
    ]
  },
  "order": [
    "args",
    "nthreads",
    "binary"
  ]
}
